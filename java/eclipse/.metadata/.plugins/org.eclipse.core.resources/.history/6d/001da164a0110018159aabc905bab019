package helloworld;

public class Student extends Person 
{
	private int year;
	private String major;
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	public String getMajor() {
		return major;
	}
	public void setMajor(String major) {
		this.major = major;
	}
	public Student(int year, String major) {
		super();
		this.year = year;
		this.major = major;
	}
	
}
